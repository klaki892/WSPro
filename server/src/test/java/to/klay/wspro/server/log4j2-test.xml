<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
    <Appenders>
        <Console name="stdout">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Logger name="to.klay.wspro" level="DEBUG" additivity="false">
            <AppenderRef ref="stdout"/>
        </Logger>
        <Root level="DEBUG" additivity="true">
            <AppenderRef ref="stdout" />
        </Root>
    </Loggers>
</Configuration>
